pipeline {
    agent any
    stages {
        stage('stage:scm') {
            steps {
                git changelog: false, poll: false, url: 'https://github.com/LuisJoseSanchez/hello-world-java.git'
            }
        }
        stage('stage:maven build') {
         steps {
            sh 'mvn clean install'
          }
        }
    }
}

